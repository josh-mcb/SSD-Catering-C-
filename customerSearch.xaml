<Window x:Class="SolutionCW.customerSearch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SolutionCW"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Events Unlimited" Height="300" Width="300">
    <Grid Background="#FF529ACD">
        <Button Content="Search" HorizontalAlignment="Left" Margin="103,225,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="searchName" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="83,115,0,0" TextChanged="searchName_TextChanged"/>
        <Label Content="Search By Lastname" HorizontalAlignment="Left" Margin="83,74,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
